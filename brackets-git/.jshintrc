{
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "quotmark": true,
    "undef": true,
